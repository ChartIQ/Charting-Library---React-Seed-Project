		"eslint-plugin-promise": "^3.4.0-0",
		"eslint-plugin-standard": "^2.0.1"
	},
	"esformatter": {
		"plugins": [
			"esformatter-jsx"
		],
		"jsx": {
			"formatJSX": true,
			"attrsOnSameLineAsTag": false,
			"maxAttrsOnTag": 3,
			"firstAttributeOnSameLine": true,
