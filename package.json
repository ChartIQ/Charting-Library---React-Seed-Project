{
	"name": "reactchartiq",
	"version": "1.0.0",
	"description": "ChartIQ Charting Library react seed project",
	"dependencies": {
		"@babel/polyfill": "^7.11.5",
		"@babel/preset-env": "^7.4.4",
		"npm": "^6.0.0",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-redux": "^5.1.2",
		"redux": "^4.0.5",
		"redux-create-action-types": "^2.1.0",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.1.2",
		"@babel/core": "^7.1.2",
		"@babel/plugin-syntax-dynamic-import": "^7.7.4",
		"@babel/preset-react": "^7.0.0",
		"babel-loader": "^8.0.4",
		"esformatter-jsx": "^8.0.1",
		"eslint": "^4.19.1",
		"eslint-config-standard": "^11.0.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^6.0.1",
		"eslint-plugin-promise": "^3.8.0",
		"eslint-plugin-standard": "^3.1.0",
		"node-sass": "^4.14.1",
		"prop-types": "^15.7.2",
		"redux-devtools": "^3.7.0",
		"webpack": "^4.23.1",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.11.0"
	},
	"esformatter": {
		"plugins": [
			"esformatter-jsx"
		],
		"jsx": {
			"formatJSX": true,
			"attrsOnSameLineAsTag": false,
			"maxAttrsOnTag": 3,
			"firstAttributeOnSameLine": true,
			"formatJSXExpressions": true,
			"JSXExpressionsSingleLine": true,
			"alignWithFirstAttribute": false,
			"spaceInJSXExpressionContainers": " ",
			"removeSpaceBeforeClosingJSX": false,
			"closingTagOnNewLine": false,
			"htmlOptions": {}
		}
	},
	"scripts": {
		"dev": "webpack --mode development",
		"scss": "node-sass --source-map true css/CIQ_Seed.scss -o css",
		"build": "webpack --mode production",
		"postinstall": "npm run dev",
		"server": "webpack-dev-server --inline --progress --port 3000 --content-base . --watch=true",
		"start": "webpack-dev-server --mode development --config=webpack.config.js --open",
		"start0": "webpack-dev-server --mode development --config=webpack.config.js --open --host 0.0.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ChartIQ/Charting-Library---React-Seed-Project.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/ChartIQ/Charting-Library---React-Seed-Project/issues"
	},
	"homepage": "https://github.com/ChartIQ/Charting-Library---React-Seed-Project#readme"
}
